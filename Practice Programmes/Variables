let age = prompt("enter your age");
let msg;

switch (true) {
    case (age >= 21):
        msg = "Bewda Sala";
        break;
    case (age >= 19 && age < 21):
        msg = "Lowde ko unde daal";
        break;
    default:
        msg = "bhag bhoshdi ke";
        break;
}

alert(msg);